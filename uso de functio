        function pesquisa(){
            window.open("http://aonde.com.br");
        }
        function diversao(){
            window.open("https://www.betocarrero.com.br/", width= 250, height =400);
        }
        function animais(){
            window.open("https://zoopomerode.com.br", width=400,height=250)
        }

    </script>
</head>

<body>
    <form> de um clique na opção pesquisa:
        <input type ="button" name="lincar"value="janela1" onclick="pesquisa()"><br><br>
        de um clique na opção diversao:
        <input type ="button" name="lincar"value="janela2" onclick="diversao()"><br><br>
        de um clique na opção animais:
        <input type ="button" name="lincar"value="janela3" onclick="animais()"><br><br>

    </form>
</body>
